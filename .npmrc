# Optimize for faster installs
prefer-offline=true
audit=false
fund=false

# Production build optimizations - Allow optional deps for Rollup
optional=true
force=false
cache-max=0

# Handle native dependencies for Vercel Linux builds
target_platform=linux
target_arch=x64

# Registry settings  
registry=https://registry.npmjs.org/

# Rollup native binary support
rollup_enable_native=true